{
    "manifest_version": 2,
    "name": "Remove Politics Youtube Homepage",
    "version": "0.1",
    
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"

            ],
            "js": [
                "jquery.js",
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    }
}